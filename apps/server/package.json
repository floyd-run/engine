{
  "name": "@floyd/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch -r dotenv/config src/index.ts",
    "test": "vitest",
    "migrate": "tsx -r dotenv/config src/scripts/migrate.ts",
    "migrate:down": "tsx -r dotenv/config src/scripts/migrate.ts down",
    "migrate:redo": "tsx -r dotenv/config src/scripts/migrate.ts redo",
    "migrate:create": "tsx -r dotenv/config src/scripts/create-migration.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint"
  },
  "dependencies": {
    "@floyd-run/schema": "workspace:*",
    "@floyd-run/utils": "workspace:*",
    "@hono/node-server": "^1.19.9",
    "hono": "^4.11.4",
    "kysely": "^0.28.9",
    "pg": "^8.17.1",
    "pino": "^10.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@floyd/eslint": "workspace:*",
    "@floyd/typescript": "workspace:*",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.28.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
